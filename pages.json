			"style": {
				"navigationBarTitleText": "首页"
			}
		}
	    ,{
            "path" : "pages/user/user",
            "style": {
				"navigationBarTitleText": "信息"
			}
        }
        ,{
            "path" : "pages/userInfo/userInfo",
            "style": {
				"navigationBarTitleText": "用户信息"
			}
        }
        ,{
            "path" : "pages/navBar/navBar",
            "style": {
            	"navigationBarTitleText": "导航栏",
